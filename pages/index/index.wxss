
.tb-toppest{
  width: 100%;
  padding-top:15rpx;
  padding-bottom: 20rpx;
  display: flex;
  align-content: center;
  justify-items: center;
  background: linear-gradient(to right, #feb275 , #feb374);
  color: #666666;
  overflow: hidden;
  padding-left: 20rpx;
  /* border-bottom-right-radius: 20rpx;
  border-bottom-left-radius: 20rpx; */
}
.tb-toppest navigator{
  flex-direction:row-reverse;
  align-items: center;
  justify-content: center;
  text-align: center;
  color: #fff;
}
.tb-toppest navigator text {
  display: block;
  line-height: 56rpx;
}
.tb-toppest navigator text.money{
  font-size: 28rpx;
}
.navigator-hover {
  width: 100%;
}
.kjContent{
  display: flex;
  align-items: center;
  justify-content: center;
}
.kjContent .btn-hover {
  width: 33.333%;
  text-align: center;
  line-height: 56rpx;
  font-size: 26rpx;
}
.kj {
  font-size: 32rpx;
  line-height: 100rpx;
  margin-left: 32rpx;
}
.kjContent{
  font-size: 28rpx;
  padding: 40rpx 0;
}
/* 顶部 */
.top{
position: relative;
display: flex;
  width: 100vw;
  padding: 0 20rpx;
  font-size:28rpx;
  border-bottom: 1px solid #f2f2f2;
}
.top-text{
  float: left
}
/* 下拉框 */
.top-selected{
    width: 20%;
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border: 1px solid #ccc;
    padding: 0 10rpx;
    font-size: 28rpx;
    justify-content: center;
}
/* 下拉内容 */
.select_box {
  background-color: #fff;
  padding: 0 20rpx;
  width: 20%;
  position: absolute;
  left: 0;
  z-index: 1;
  overflow: hidden;
  text-align: left;
  animation: myfirst 0.5s;
  font-size: 28rpx;
  top: 75rpx;
}
.select_one {
    padding-left: 20rpx;
    height: 60rpx;
    position: relative;
    line-height: 60rpx;
    border-bottom: 1px solid #f3f3f3;
}
/* 下拉过度效果 */
@keyframes myfirst {
    from {
        height: 0rpx;
    }
 
    to {
        height: 210rpx;
    }
}
/* 下拉图标 */
.top-selected image{
  height:36rpx;
  width:36rpx;
}

.banner {
  display: block;
  overflow: hidden;
  width: 750rpx;
  height: 308rpx;
}
.search{
  width: 60%;
}
.search_arr {
  display: flex;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  margin-left: 20rpx;
}
.search_arr input{
  margin-left: 20rpx;
  height: 60rpx;
  border-radius: 5px;
}
.bc_text {
  line-height: 68rpx;
  height: 68rpx;
  margin-top: 34rpx;
}
 
.sousuo {
  margin-left: 15rpx;
  width: 18%;
  line-height: 54rpx;
  text-align: center;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
}
.page_row{
  display: flex;
  width: 100%;
  align-items: center;
}
.searchcion {
  margin: 10rpx 10rpx 10rpx 10rpx;
  width:20rpx;
  height: 20rpx;
  text-align: center;
}  
.indexIcon {
  margin: 10rpx 0;
}

.indexIcon navigator {
  display: inline-block;
  width: 33.3333%;
  margin: 0 auto 20rpx;
  height: 160rpx;
  text-align: center;
}

.iconfont {
  display: flex;
  margin: 0 auto;
  text-align: center;
  align-items: center;
  justify-content: center;
  width: 80rpx;
  height: 80rpx;
  border-radius: 40%;
  box-shadow: 0 4px 8px rgba(26, 0, 173, 0.2);
  -webkit-box-shadow: 0 4px 8px rgba(26, 0, 173, 0.2);
}

.iconBox {
  background: -webkit-linear-gradient(bottom, #9faffc 0%, #617cfd 80%, #5d7cfc 100%);
  background: linear-gradient(to bottom, #9faffc 0%, #617cfd 80%, #5d7cfc 100%);
}
.indexIcon navigator .iconOrder {
  background: -webkit-linear-gradient(top, #fc9f9f 0%, #fd6161 80%, #fc5d5d 100%);
  background: linear-gradient(top, #fc9f9f 0%, #fd6161 80%, #fc5d5d 100%);
}
.indexIcon navigator image {
  width: 80rpx;
  height: 80rpx;
  display: block;
  margin: 0 auto;
}
.indexIcon navigator text {
  font-size: 26rpx;
}
.indexIcon navigator .iconOrder {
  background: -webkit-linear-gradient(top, #fc9f9f 0%, #fd6161 80%, #fc5d5d 100%);
  background: linear-gradient(top, #fc9f9f 0%, #fd6161 80%, #fc5d5d 100%);
}
.typeIn {
  background: -webkit-linear-gradient(bottom, #ffad72 0%, #ff8463 80%, #ff6356 100%);
  background: linear-gradient(to bottom, #ffad72 0%, #ff8463 80%, #ff6356 100%);
}

.divLine{
  background: #E0E3DA;
  width: 100%;
  height: 2rpx;
  margin-bottom: 10px;
  margin-top: 5px;
 }